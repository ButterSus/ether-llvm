// TODO: Specify bit depth

#ifndef ELF_RELOC
#error "ELF_RELOC must be defined"
#endif

// No relocation
ELF_RELOC(R_ETHER_NONE,            0)
// General 32-bit relocation
ELF_RELOC(R_ETHER_32,              1)
// (Might be not used)
ELF_RELOC(R_ETHER_RELATIVE,        2)
// ...-bit branch targets
ELF_RELOC(R_ETHER_BRANCH,          3)
// ...-bit call targets
ELF_RELOC(R_ETHER_CALL,            4)
// Upper 16-bits of a symbolic relocation
ELF_RELOC(R_ETHER_HI16,            5)
// Lower 16-bits of a symbolic relocation
ELF_RELOC(R_ETHER_LO16,            6)
// Program counter 16-bit relative relocation
ELF_RELOC(R_ETHER_PCREL16,         7)
// Program counter 32-bit relative relocation
ELF_RELOC(R_ETHER_PCREL32,         8)
